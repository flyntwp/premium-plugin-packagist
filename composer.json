{
    "name": "flyntwp/premium-plugin-packagist",
    "description": "Premium Plugin Packagist helps you to create a packagist / satis server to provide plugins retrieved with the Premium Plugin Downloader.",
    "type": "library",
    "license": "MIT",
    "bin": ["bin/premium-plugin-packagist"],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "composer/satis": "dev-master",
        "symfony/console": "^4.1",
        "guzzlehttp/promises": "^1.3",
        "webmozart/path-util": "^2.3",
        "nabil1337/case-helper": "^0.1.0",
        "vlucas/phpdotenv": "^2.4",
        "flyntwp/premium-plugin-downloader": "dev-master"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.9.1 || ^3.3.1"
    },
    "repositories": [
        {
            "url": "https://github.com/flyntwp/premium-plugin-downloader.git",
            "type": "git"
        }
    ],
    "autoload": {
        "psr-4": {"FlyntWP\\PremiumPluginPackagist\\": "src/"}
    }
}
